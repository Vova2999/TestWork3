{
	"AppSettings": {
		"EnvironmentVariableKeys": [
			"API_BASE_URL",
			"API_TIMEOUT",
			"API_LOGIN",
			"API_PASSWORD"
		]
	},
	"NLog": {
		"throwConfigExceptions": true,
		"targets": {
			"async": true,
			"logfile": {
				"type": "File",
				"fileName": "logs/test-sms-wpf-app-${date:yyyyMMdd}.log"
			}
		},
		"rules": [
			{
				"logger": "*",
				"minLevel": "Info",
				"writeTo": "logfile"
			}
		]
	}
}